{
  "version": 2,
  "builds": [
    {
      "src": "/app/app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },

    {
      "src": "build_files.sh",
      "use": "@now/static-build"
    }

  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/app.py"
    }
  ]
  }